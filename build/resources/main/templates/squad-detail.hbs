{{#partial "content"}}
    {{#each squad}}
        <div class="squad-part">
            <a href="/squad-form">Add new Squad to list</a>
            <p>Name:{{ name }}</p>
            <p>Size:{{ size }}</p>
            <p>Cause:{{ cause }}</p>
            <p>

                {{#if post.published }}
                    <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published

                {{/if}}
            </p>
        </div>
    {{/each}}
{{/partial}}

{{> layout.hbs}}